{
  "name": "cpdrenato/screen-64bits",
  "description": "A PHP Class to interact with PhantomJs and capture screenshot of a webpage",
  "keywords": [
    "phantomjs",
    "screen",
    "capture",
    "print-screen"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Renato Lucena",
      "email": "cpdrenato@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "symfony/var-dumper": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Screen\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "chmod +x bin/phantomjs"
    ]
  }
}
